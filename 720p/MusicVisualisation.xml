<?xml version="1.0" encoding="UTF-8"?>
<window id="2006">
    <onload condition="System.HasAddon(script.artistslideshow) + Skin.HasSetting(HomeMusicFanart)">RunScript(script.artistslideshow)</onload>
    <defaultcontrol always="true">999</defaultcontrol>
    <controls>
        <control type="visualisation" id="2">
            <description>visualisation</description>
            <left>0</left>
            <top>0</top>
            <width>1280</width>
            <height>720</height>
        </control>
        <control type="image">
            <description>Default background</description>
            <left>0</left>
            <top>0</top>
            <width>1280</width>
            <height>720</height>
            <colordiffuse>88FFFFFF</colordiffuse>
            <visible>!Skin.HasSetting(CustomBackground) + !Skin.HasSetting(BlackBackground) + !Skin.HasSetting(Artistslideshow) + !Skin.HasSetting(CDWallNoVisualisation)</visible>
            <texture fallback="special://skin/backgrounds/DefaultFanart.jpg">$INFO[MusicPlayer.Property(Fanart_Image)]</texture>
            <include condition="Skin.HasSetting(animation_bg_global)">global_kenburns_fanart</include>
        </control>
        <control type="multiimage">
            <left>0</left>
            <top>0</top>
            <width>1280</width>
            <height>720</height>
            <imagepath fallback="special://skin/backgrounds/musicfanart.jpg" background="true">$INFO[Window.Property(ArtistSlideshow)]</imagepath>
            <aspectratio>keep</aspectratio>
            <timeperimage>5000</timeperimage>
            <colordiffuse>88FFFFFF</colordiffuse>
            <fadetime>2000</fadetime>
            <randomize>true</randomize>
            <animation effect="fade" start="0" end="100" time="300">Visible</animation>
            <animation effect="fade" start="100" end="0" time="300">Hidden</animation>
            <animation effect="zoom" start="110" end="130" center="auto" time="6000" pulse="true" condition="Skin.HasSetting(ArtistslideshowAnimation)">Conditional</animation>
            <animation effect="slide" start="-20,-20" end="40,40" time="7000" pulse="true" condition="Skin.HasSetting(ArtistslideshowAnimation)">Conditional</animation>
            <visible>IsEmpty(Window.Property(ArtistSlideshowRefresh))</visible>
            <visible>Skin.HasSetting(Artistslideshow)</visible>
            <visible>!Skin.HasSetting(BlackBackground)</visible>
            <visible>!Skin.HasSetting(CustomBackground)</visible>
            <visible>!Skin.HasSetting(DefaultBackground)</visible>
        </control>
        <control type="multiimage">
            <description>Custom background</description>
            <left>0</left>
            <top>0</top>
            <width>1280</width>
            <height>720</height>
            <fadetime>2000</fadetime>
            <timeperimage>5000</timeperimage>
            <colordiffuse>88FFFFFF</colordiffuse>
            <visible>Skin.HasSetting(CustomBackground) + !Skin.HasSetting(BlackBackground) + !Skin.HasSetting(DefaultBackground) + !Skin.HasSetting(Artistslideshow)</visible>
            <imagepath>$INFO[Skin.String(Home_Custom_Back_MusicVisualisation_Folder)]</imagepath>
            <include condition="Skin.HasSetting(animation_bg_global)">global_kenburns_fanart</include>
        </control>
        <control type="image">
            <description>Black background</description>
            <left>0</left>
            <top>0</top>
            <width>1280</width>
            <height>720</height>
